{
  "name": "react-uikit-components",
  "version": "1.2.0",
  "description": "React UIkit components for UIkit CSS framework.",
  "keywords": [
    "javascript",
    "react",
    "uikit",
    "react-component",
    "react-uikit",
    "uikit-react"
  ],
  "repository": "git@github.com:otissv/react-uikit-components.git",
  "author": "Otis Virginie",
  "license": "MIT",
  "main": "./docs/scripts/bundle.js",
  "scripts": {
    "build": "webpack --config ./webpack.config-build.js --progress",
    "lint": "eslint --quiet ./src/**  && date || true && date",
    "lint:watch": "watch 'npm run lint' ./",
    "uikit": "postcssbuild --config postcssbuild.json --d src/styles/uikit -o docs/styles/uikit.min.css",
    "css": "postcssbuild --config postcssbuild.json --s src/styles/main.css -o docs/styles/main.min.css",
    "css:build": "npm run uikit && npm run css",
    "prepublish": "npm run build && npm run css:build",
    "start": "node ./server.js",
    "webpack": "webpack",
    "watch": "webpack --watch"
  },
  "devDependencies": {
    "babel-cli": "~6.8.0",
    "babel-core": "~6.8.0",
    "babel-eslint": "~6.0.4",
    "babel-loader": "~6.2.4",
    "babel-plugin-react-transform": "~2.0.2",
    "babel-plugin-transform-runtime": "~6.8.0",
    "babel-preset-es2015": "~6.6.0",
    "babel-preset-react": "~6.5.0",
    "babel-preset-stage-0": "~6.5.0",
    "babel-runtime": "~6.6.1",
    "babelify": "~7.3.0",
    "browser-sync": "~2.12.5",
    "cheerio": "~0.20.0",
    "eslint": "~2.9.0",
    "eslint-config-standard": "~5.3.0",
    "eslint-loader": "~1.3.0",
    "eslint-plugin-promise": "~1.1.0",
    "eslint-plugin-react": "~5.0.1",
    "eslint-plugin-smells": "~1.0.1",
    "eslint-plugin-standard": "~1.3.2",
    "faucet": "~0.0.1",
    "history": "~2.1.1",
    "json-loader": "~0.5.4",
    "postcss-build": "^1.0.6",
    "react-addons-test-utils": "~15.0.2",
    "react-dom": "~15.0.2",
    "react-transform-catch-errors": "~1.0.2",
    "react-transform-hmr": "~1.0.4",
    "redbox-react": "~1.2.4",
    "tap-difflet": "~0.4.0",
    "tap-min": "~1.1.0",
    "tape": "~4.5.1",
    "uglify-loader": "~1.3.0",
    "watch": "~0.18.0",
    "webpack": "~1.13.0",
    "webpack-dev-middleware": "~1.6.1",
    "webpack-hot-middleware": "~2.10.0"
  },
  "dependencies": {
    "cuid": "~1.3.8",
    "history": "~2.1.1",
    "react": "~15.0.2",
    "react-dom": "~15.0.2",
    "react-router": "~2.4.0",
    "react-uikit-alert": "latest",
    "react-uikit-article": "latest",
    "react-uikit-badge": "latest",
    "react-uikit-base": "latest",
    "react-uikit-block": "latest",
    "react-uikit-breadcrumb": "latest",
    "react-uikit-button": "^1.1.0",
    "react-uikit-button-group": "latest",
    "react-uikit-codeblock": "latest",
    "react-uikit-comment": "latest",
    "react-uikit-comment-list": "latest",
    "react-uikit-dropdown": "latest",
    "react-uikit-flex": "latest",
    "react-uikit-form": "latest",
    "react-uikit-grid": "latest",
    "react-uikit-icons": "latest",
    "react-uikit-img": "latest",
    "react-uikit-img-gallery": "latest",
    "react-uikit-list": "latest",
    "react-uikit-modal": "latest",
    "react-uikit-nav": "latest",
    "react-uikit-note": "latest",
    "react-uikit-overlay": "latest",
    "react-uikit-panel": "latest",
    "react-uikit-placeholder": "latest",
    "react-uikit-table": "latest",
    "react-uikit-text": "latest",
    "react-uikit-thumbnail": "latest",
    "velocity-animate": "~1.2.3"
  },
  "engines": {
    "node": "~4.4.3"
  }
}
